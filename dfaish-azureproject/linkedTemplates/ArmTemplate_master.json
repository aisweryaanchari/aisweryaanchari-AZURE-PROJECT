{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfaish-azureproject"
		},
		"LS_ADLSGEN2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSGEN2'"
		},
		"ls_az_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_az_sql'"
		},
		"LS_ADLSGEN2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://aishstorageazureproject.dfs.core.windows.net/"
		},
		"ls_RESTAPIPR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/"
		},
		"ls_az_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "aishwserver.database.windows.net"
		},
		"ls_az_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbaish"
		},
		"ls_az_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "aiswerya"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfaish-azureproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSGEN2_accountKey": {
						"value": "[parameters('LS_ADLSGEN2_accountKey')]"
					},
					"ls_az_sql_password": {
						"value": "[parameters('ls_az_sql_password')]"
					},
					"LS_ADLSGEN2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSGEN2_properties_typeProperties_url')]"
					},
					"ls_RESTAPIPR_properties_typeProperties_url": {
						"value": "[parameters('ls_RESTAPIPR_properties_typeProperties_url')]"
					},
					"ls_az_sql_properties_typeProperties_server": {
						"value": "[parameters('ls_az_sql_properties_typeProperties_server')]"
					},
					"ls_az_sql_properties_typeProperties_database": {
						"value": "[parameters('ls_az_sql_properties_typeProperties_database')]"
					},
					"ls_az_sql_properties_typeProperties_userName": {
						"value": "[parameters('ls_az_sql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}